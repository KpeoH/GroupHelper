<Application x:Class="WPFinal.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:WPFinal"
             xmlns:converters="clr-namespace:WPFinal.ViewModels.Converters"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            
            
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Themes/LightTheme.xaml"/>
            </ResourceDictionary.MergedDictionaries>
            
            
            
            <converters:SubjectsConverter x:Key="SubjectsConverter" />
            
            <converters:StudentsConverter x:Key="StudentsConverter" />
            
            
        </ResourceDictionary>
    </Application.Resources>
</Application>
